@{
    Layout = "~/Views/Shared/_LayoutTurnkeypages.cshtml";
    ViewData["Title"] = "Our Services";
}


<!-- Page Header Start -->
<div class="container-fluid page-header pt-5 mb-6 wow fadeIn" data-wow-delay="0.1s">
    <div class="container text-center pt-5">
        <div class="row justify-content-center">
            <div class="col-lg-7">
                <div class="bg-white p-5">
                    <h1 class="display-6 text-uppercase mb-3 animated slideInDown">Air Handling Units</h1>
                    <nav aria-label="breadcrumb animated slideInDown">
                        <ol class="breadcrumb justify-content-center mb-0">
                            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
                            <li class="breadcrumb-item" aria-current="page">Air Handling Units</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Page Header End -->
<!-- Air Handling Unit Details Page -->
<div class="container-fluid pt-6 pb-6">
    <div class="container">
        <div class="row g-5">
            <div class="col-lg-12 wow fadeIn" data-wow-delay="0.1s">
                <h1 class="display-6 text-uppercase mb-4">Air Handling Units (AHUs)</h1>
                <p class="mb-4">
                    Our Air Handling Units are engineered to deliver optimum performance in pharmaceutical, cleanroom, and industrial HVAC applications. Designed with modular construction and high-grade components, our AHUs ensure energy efficiency, reliability, and ease of maintenance.
                </p>
                <table class="table table-bordered bg-white">
                    <thead class="table-primary text-uppercase">
                        <tr>
                            <th>Description</th>
                            <th>Specification</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Capacity</td>
                            <td>500 to 65,000 CFM in Horizontal/Vertical Floor and Ceiling Suspended Units</td>
                        </tr>
                        <tr>
                            <td>Framework</td>
                            <td>Hollow Extruded Aluminum Profiles (with thermal break and internal coving)</td>
                        </tr>
                        <tr>
                            <td>Blowers</td>
                            <td>Forward/Backward Plug type/EC Fans</td>
                        </tr>
                        <tr>
                            <td>Evaporator Coil</td>
                            <td>Copper tubes with Aluminium fins (Anti-microbial coating available)</td>
                        </tr>
                        <tr>
                            <td>Filtration</td>
                            <td>Pre/Fine/Carbon/HEPA Filtration (EU 7/9/13)</td>
                        </tr>
                        <tr>
                            <td>Panel Construction</td>
                            <td>Double/Single Skin GI Pre-coated/Powder Coated/SS filled with insulation</td>
                        </tr>
                        <tr>
                            <td>Insulation</td>
                            <td>Poly-Urethane Foam (PUF) (up to 40 kg/m³)/High Density Rockwool</td>
                        </tr>
                        <tr>
                            <td>Panel Options</td>
                            <td>25mm & 45mm Thermal and Non-Thermal Break</td>
                        </tr>
                        <tr>
                            <td>Vibration Isolators</td>
                            <td>Blower mechanics mounted on rubber isolators for performance</td>
                        </tr>
                        <tr>
                            <td>Energy Recovery Unit</td>
                            <td>More than 75% recovery from exhaust (BryAir)</td>
                        </tr>
                        <tr>
                            <td>Silent Air</td>
                            <td>Sound attenuation for low-noise applications</td>
                        </tr>
                        <tr>
                            <td>UVC Stage</td>
                            <td>Improving the IAQ with UV A/B/C (380 - 315/315 - 280/280 - 100 nm)</td>
                        </tr>
                        <tr>
                            <td>Smart AHU</td>
                            <td>Monitoring and control of parameters including VFD for fans</td>
                        </tr>
                        <tr>
                            <td>Accessories</td>
                            <td>Access Door with hinges, handles and doorknobs; Polycarbonate View-glass; Pressure ports; DOP Ports</td>
                        </tr>
                    </tbody>
                </table>
                <div class="mt-4 text-end">
                    <a href="/products" class="btn btn-primary px-4 py-2">Back to Products</a>
                </div>
            </div>
        </div>
    </div>
</div>

