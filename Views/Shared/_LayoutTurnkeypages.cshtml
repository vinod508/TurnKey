    <div class="container-fluid bg-white sticky-top wow fadeIn" data-wow-delay="0.1s">
        <div class="container">
            <nav class="navbar navbar-expand-lg bg-white navbar-light p-lg-0">
                <a href="index.html" class="navbar-brand d-lg-none">
                    <h1 class="fw-bold m-0">TurnKey</h1>
                </a>
                <button type="button" class="navbar-toggler me-0" data-bs-toggle="collapse"
                        data-bs-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <div class="navbar-nav">
                        <a asp-controller="Home" asp-action="Index" class="nav-item nav-link">Home</a>
                        <div class="nav-item dropdown">
                            <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Who We Are</a>
                            <div class="dropdown-menu bg-light rounded-0 rounded-bottom m-0">
                               
                                <a asp-controller="Home" asp-action="AboutUs" class="dropdown-item">About</a>
                                <a asp-controller="Home" asp-action="OurTeam" class="dropdown-item">Our Team</a>
                               
                                <a asp-controller="Home" asp-action="Project" class="dropdown-item">Our Projects</a>
                                
                            </div>
                        </div>
                        <a asp-controller="Home" asp-action="OurServices" class="nav-item nav-link active">Services</a>
                      
                        <a asp-controller="Home" asp-action="ContactUs" class="nav-item nav-link">Contact</a>
                    </div>
                    <div class="ms-auto d-none d-lg-block">
                        <a href="" class="btn btn-primary py-2 px-3">Get A Quote</a>
                    </div>
                </div>
            </nav>
        </div>
    </div>
