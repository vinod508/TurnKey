    </div>
    <!-- Spinner End -->
    <!-- Topbar Start -->
    <div class="container-fluid bg-primary text-white d-none d-lg-flex wow fadeIn" data-wow-delay="0.1s">
        <div class="container py-3">
            <div class="d-flex align-items-center">
                <a href="index.html">
                    <h2 class="text-white fw-bold m-0">TurnKey</h2>
                </a>
                <div class="ms-auto d-flex align-items-center">
                    <small class="ms-4">
                        <i class="fa fa-map-marker-alt me-3"></i>A-158,
                        Wagle Indl. Thane-Maharshtra - 400604
                    </small>
                    <small class="ms-4"><i class="fa fa-envelope me-3"></i>account2@snehalacr.com</small>
                    <small class="ms-4"><i class="fa fa-phone-alt me-3"></i>+91 22 4068 1414 / 15</small>
